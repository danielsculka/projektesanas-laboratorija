{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Debug",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithEnvironmentName"
    ],
    "Properties": {
      "Application": "ProLab.Api"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=localhost,1433;Database=ProLab;MultipleActiveResultSets=true;TrustServerCertificate=True;Integrated security=false;User ID=sa;Password=Pass@word;Connect Timeout=60;"
  },
  "OpenRoute": {
    "Url": "https://api.openrouteservice.org/v2"
  }
}
