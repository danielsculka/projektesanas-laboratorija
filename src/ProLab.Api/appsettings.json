{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithEnvironmentName"
    ],
    "Properties": {
      "Application": "ProLab.Api"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "__REQUIRED__"
  },
  "App": {
    "Url": "https://__REQUIRED__"
  },
  "OpenRoute": {
    "Url": "https://api.openrouteservice.org/v2",
    "UserToken": "__REQUIRED__"
  },
  "Swagger": {
    "Path": "/docs"
  }
}
